{
  "name": "grabbit",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "description": "An Electron-based application built with React, TypeScript, and Vite",
  "author": "Klaus",
  "license": "MIT",
  "homepage": "https://github.com/MrMuscleFreak/grabbit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrMuscleFreak/grabbit.git"
  },
  "bugs": {
    "url": "https://github.com/MrMuscleFreak/grabbit/issues"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "vite",
    "desktop-app"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build:dir": "npm run build -- --dir",
    "build:win": "npm run build -- --win",
    "build:mac": "npm run build -- --mac",
    "build:linux": "npm run build -- --linux",
    "electron": "electron .",
    "electron:dev": "vite build && electron .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "clean": "rm -rf dist dist-electron",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "electron-log": "^5.4.1",
    "electron-store": "^10.1.0",
    "framer-motion": "^12.23.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
